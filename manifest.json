{
  "description": "YouTube title untranslator",
  "manifest_version": 2,
  "name": "YouTube untranslate",
  "version": "1.0.1",
  "icons": {
    "48": "icons/48x48.png",
    "96": "icons/96x96.png"
  },

  "content_scripts": [
    {
      "matches": ["https://*.youtube.com/watch?*"],
      "js": ["dist/content.js"],
      "all_frames": true
    }
  ]
}